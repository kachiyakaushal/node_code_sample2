{{!-- Profile Starts --}}
<div class="m-portlet m-4 borderradiusbox lighttxtcolor">
   <form id="my_account" method="POST" action="/my-account"  class="m-form m-form--fit1">
   <input type="hidden" name="userId" value="{{data.userId}}" />
   <div class="m-portlet__head">
      <div class="m-portlet__head-caption">
         {{#if data.profilePictureLink}}
         <img src="{{data.profilePictureLink}}" id="customFileImg" class="ppimgsmall rounded-circle d-block m-auto" alt="">
         {{/if}}
         <div class="cameraicon align-self-end">
            <input type="file" name="profilePicture" class="custom-file-input hide" onchange="updateULogo(this, 'customFileImg')" id="customFile">
            <label for="customFile">
               <i class="la la-camera"></i>
            </label>
         </div>
         <div class="ml-3">            
            <div class="d-block font-s-12 fw-600 mb-1">{{data.firstName}} {{data.lastName}}</div>
            <div class="d-block fw-500">
               {{accountroles data.role}}
            </div>
         </div>         
      </div>
   </div>
   <div class="m-portlet__body bgwhitelight brbottomlr">
      <div class="form-group m-form__group1 row">
         <div class="col-md-4">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="username">Username</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="text" name="username" value="{{data.username}}" id="username" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right"><span><i class="la la-pencil"></i></span></span>
                  </div>                  
               </div>
            </div>
         </div>
         <div class="col-md-4 mob-mt-3">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="firstName">First Name</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="text" name="firstName" value="{{data.firstName}}" id="firstName" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right"><span><i class="la la-pencil"></i></span></span>
                  </div>                  
               </div>
            </div>
         </div>
         <div class="col-md-4 mob-mt-3">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="lastName">Last Name</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="text" name="lastName" value="{{data.lastName}}" id="lastName" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right"><span><i class="la la-pencil"></i></span></span>
                  </div>                  
               </div>
            </div>
         </div>
      </div>
      <div class="form-group m-form__group1 row mt-md-4">
         <div class="col-md-4">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="email">Email</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="text" name="email" value="{{data.email}}" id="email" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right"><span><i class="la la-pencil"></i></span></span>
                  </div>                  
               </div>
            </div>
         </div>
         <div class="col-md-4 mob-mt-3">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="phone">Phone</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="text" name="phone" value="{{data.phone}}" id="phone" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right"><span><i class="la la-pencil"></i></span></span>
                  </div>                  
               </div>
            </div>
         </div>         
      </div>
      <div class="row mt-5 align-items-center">
         <div class="col-lg-12 m--valign-middle">
            <button type="submit" class="btn btn-primary m-btn m-btn--custom fw-600">Update</button>
         </div>
      </div>
   </div>
   </form>
</div>
{{!-- Profile Ends --}}

{{!-- Reset Password Starts --}}
<div class="m-portlet m-4 mt-5 borderradiusbox lighttxtcolor">
   <form id="my_account_two" method="POST" action="/my-account/changepassword"  class="m-form m-form--fit1">
   <input type="hidden" name="userId" value="{{data.userId}}" />
   <div class="m-portlet__head">
      <div class="m-portlet__head-caption">
         <div class="">            
            <div class="d-block font-s-12 fw-600">Reset Password</div>
         </div>         
      </div>
   </div>
   <div class="m-portlet__body bgwhitelight brbottomlr">
      <div class="form-group m-form__group1 row">
         <div class="col-md-4">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="currentPassword">Current Password</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="password" name="password" id="currentPassword" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right viewpassword"><span><i class="la la-eye"></i></span></span>
                  </div>
               </div>
            </div>
         </div>
         <div class="col-md-4 mob-mt-3">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="newPassword">New Password</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="password" name="newPassword" id="newPassword" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right viewpassword"><span><i class="la la-eye"></i></span></span>
                  </div>
               </div>
            </div>
         </div>
         <div class="col-md-4 mob-mt-3">
            <div class="row align-items-center">
               <div class="col-12 col-xl-4">
                  <label class="m-xl-0" for="confirmPassword">Confirm Password</label>
               </div>
               <div class="col">
                  <div class="m-input-icon m-input-icon--right">
                     <input type="password" name="confirmPassword" id="confirmPassword" class="form-control m-input" placeholder="">
                     <span class="m-input-icon__icon m-input-icon__icon--right viewpassword"><span><i class="la la-eye"></i></span></span>
                  </div>                  
               </div>
            </div>
         </div>
      </div>
      <div class="row mt-5 align-items-center">
         <div class="col-lg-12 m--valign-middle">
            <button type="submit" class="btn btn-primary m-btn m-btn--custom fw-600">Reset Password</button>
         </div>
      </div>
   </div>
   </form>
</div>
{{!-- Contact Ends --}}