                  </th>
                  <th>{{arrowUpLocal 2 "asc" "sort_transaction" }} {{arrowDownLocal 2 "desc" "sort_transaction" }} ID</th>
                  <th>{{arrowUpLocal 3 "asc" "sort_transaction" }} {{arrowDownLocal 3 "desc" "sort_transaction" }} Transaction Reference No</th>
                  <th>{{arrowUpLocal 4 "asc" "sort_transaction" }} {{arrowDownLocal 4 "desc" "sort_transaction" }} Name</th>
                  <th>{{arrowUpLocal 5 "asc" "sort_transaction" }} {{arrowDownLocal 5 "desc" "sort_transaction" }} Date</th>
                  <th>{{arrowUpLocal 6 "asc" "sort_transaction" }} {{arrowDownLocal 6 "desc" "sort_transaction" }} Amount</th>
                  <th>Service</th>
                  {{!-- <th>ImageUrl</th> --}}
                  {{!-- <th>Image</th> --}}
                  {{!-- <th class="excludeactions"></th> --}}
               </tr>
            </thead>
            <tbody>
               {{#if data.ConsumerTransactions}}
               {{#each data.ConsumerTransactions }}
               <tr class="rowitem excludethis">
                  <td class="align-middle excludecheckbox">
                     <label class="m-checkbox m-checkbox--bold m-checkbox--square valignsuper">
                        <input type="checkbox" class="commoncheck">
                        <span></span>
                     </label>
                  </td>
                  <td class="align-middle">{{id}}</td>
                  <td class="align-middle">{{transactionId}}</td>
                  <td class="align-middle">                     
                     {{User.firstName}} {{User.lastName}} ({{User.username}})                    
                  </td>
                  <td class="align-middle">{{moment transactionDate format="MM/DD/YYYY"}}</td>
                  <td class="align-middle">{{points}}</td>
                  <td class="align-middle mxwpx-400">
                     {{#if serviceArray}}                     
                        {{#each serviceArray }} 
                           <span class="m-badge m-badge--metal m-badge--wide m-badge--rounded mb-1">{{serviceName}}</span>
                        {{/each}}
                     {{/if}}                     
                  </td>
                       <td class="align-middle actiontd">
                          {{#ifCond pointImageUrl "!=" null}}
                        <a class="cur-pointer" data-toggle="modal" data-target="#vendorImageModal" data-imageurl="{{pointImageUrl}}"><i class="fa fa-paperclip"></i></a>
                        {{/ifCond}}
                     </td>
               </tr>
               {{/each}}
               {{else}}
               <tr>
                  <td colspan="10" class="text-center p-4">No records found</td>
               </tr>
               {{/if}}               
            </tbody>
         </table>
      </div>
   </div>
</div>
{{!-- Transaction History Ends --}}
