<div class="m-portlet m-4 mt-5 borderradiusbox lighttxtcolor">
   <form id="shop_documents" method="POST" action="/vendors/editdata"  class="m-form m-form--fit1">
   <input type="hidden" name="shopId" value="{{data.shopId}}" />
   <div class="m-portlet__head">
      <div class="m-portlet__head-caption">
         <div class="">            
            <div class="d-block font-s-12 fw-600">Document Upload</div>
         </div>         
      </div>
   </div>
   <div class="m-portlet__body bgwhitelight brbottomlr">
      <div class="row">
         <div class="col-12 col-lg-6 filelistsdiv">
            {{#if data.ShopDocuments}}
               {{#each data.ShopDocuments }}
               <div class="filelistbox">
                  <div class="row align-items-center ">
                  <div class="col m--valign-middle">
                     <div class="fw-500 font-s-12">
                        <a href="{{docUrl}}" target="_blank">{{name}}</a>
                     </div>
                  </div>
                  <div class="col-auto m--valign-middle text-right actiontd">
                     <a href="/vendors/docupload/{{shopId}}/delete/{{docId}}" class="deletealert"><i class="fa fa-times"></i></a>
                  </div>
                  </div>
               </div>
               {{/each}}
            {{else}}
               <div class="nodocsmsg text-center">
                  <h5 class="mb-0 lighttxtcolor">No documnets uploaded</h5>
               </div>
            {{/if}}
         </div>
         <div class="col-12 col-lg-6 mt-lg-0 mt-md-4">
            <div class="m-dropzone dropzone" action="/vendors/docupload/{{data.shopId}}/addnew" id="m-dropzone-one">
               <div class="m-dropzone__msg dz-message needsclick">
                  <div class="dropupldiv d-block mb-2">
                     <i class="la la-cloud-upload"></i>
                  </div>
                  <h5 class="mb-0">Drag and drop a file here or click to upload</h5>
               </div>
            </div>
         </div>
      </div>
   </div>
   </form>
</div>
{{!-- Document Upload Ends --}}

{{!-- Account Role Starts --}}
<div class="m-portlet m-4 mt-5 borderradiusbox lighttxtcolor">
   <input type="hidden" name="shopId" value="{{data.shopId}}" />
   <div class="m-portlet__head">
      <div class="m-portlet__head-caption">
         <div class="">            
            <div class="d-block font-s-12 fw-600">Account Role</div>
         </div>         
      </div>
      <div class="m-portlet__head-tools">
         {{!-- <a class="btn btn-success whitefont m-btn m-btn--custom fw-600 mr-3 ">Add Account</a> --}}
         <a href="/vendors/accountrole/add/{{data.shopId}}" class="btn btn-success whitefont m-btn m-btn--custom fw-600">Create Account</a>
      </div>
   </div>
   <div class="m-portlet__body bgwhitelight brbottomlr">
      <div class="cus-table noborder table-responsive lighttxtcolor">
         <table class="table m-table table-hover medwaction" id="sort_ctbl">
            <thead class="thead-inverse">
               <tr>
                  <th>Full Name</th>
                  <th>{{arrowUpLocal 2 "asc" "sort_ctbl" }} {{arrowDownLocal 2 "desc" "sort_ctbl" }} Username</th>
                  <th>{{arrowUpLocal 3 "asc" "sort_ctbl" }} {{arrowDownLocal 3 "desc" "sort_ctbl" }} E-mail</th>
                  <th>{{arrowUpLocal 4 "asc" "sort_ctbl" }} {{arrowDownLocal 4 "desc" "sort_ctbl" }} Phone</th>
                  <th>{{arrowUpLocal 5 "asc" "sort_ctbl" }} {{arrowDownLocal 5 "desc" "sort_ctbl" }} Role</th>
                  <th></th>
               </tr>
            </thead>
            <tbody>
               {{#if data.VendorShops}}
               {{#each data.VendorShops }}
               <tr class="rowitem">
                  <td class="align-middle">{{User.firstName}} {{User.lastName}}</td>
                  <td class="align-middle">{{User.username}}</td>
                  <td class="align-middle">{{User.email}}</td>
                  <td class="align-middle">{{User.phone}}</td>
                  <td class="align-middle">{{accountroles User.role}}</td>
                  <td class="align-middle actiontd">
                     <a href="/vendors/{{../data.shopId}}/accountrole/edit/{{User.userId}}" class=""><i class="fa fa-pencil-alt"></i></a>
                  </td>
               </tr>
               {{/each}}
               {{else}}
               <tr>
                  <td colspan="10" class="text-center p-4">No records found</td>
               </tr>
               {{/if}}
            </tbody>
         </table>
      </div>
   </div>
</div>
{{!-- Account Role Ends --}}

